{
    "name" : "miklcct/rail_open_timetable_data",
    "description" : "Library for handling rail open timetable data",
    "type" : "library",
    "license" : "MIT",
    "require" : {
        "php" : "^8.1",
        "thecodingmachine/safe": "^2.1",
        "mongodb/mongodb": "^1.12",
        "ext-mongodb" : "*",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^6.1"
    },
    "require-dev" : {
        "roave/security-advisories" : "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "autoload" : {
        "psr-4" : {
            "Miklcct\\RailOpenTimetableData\\" : "src/"
        },
        "files" : [
            "src/functions.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Test\\Miklcct\\RailOpenTimetableData\\" : "test/"
        }
    }
}
